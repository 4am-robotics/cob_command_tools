  <depend stack="pr2_common" /> <!-- pr2_msgs -->
  <depend stack="pr2_controllers" /> <!-- pr2_controllers_msgs -->
  <depend stack="rqt_gui" /> <!-- rqt_gui -->
  <depend stack="rqt_pr2_dashboard" /> <!-- rqt_pr2_dashboard -->
