<package>
  <description brief="cob_ui_but">

COB visualization tools provided by dcgm-robotics@FIT group:
- RViz plugin for COB arm navigation
- COB teleop interactive marker

  </description>
  <author>Zdenek Materna/imaterna@fit.vutbr.cz, Tomas Lokaj, Michal Spanel/spanel@fit.vutbr.cz</author>
  <license>LGPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/cob_ui_but</url>

  <depend package="roscpp"/>
  <depend package="visualization_msgs"/>
  <depend package="interactive_markers"/>
  <depend package="std_msgs"/>
  <depend package="rviz"/>
  <depend package="ogre"/>
  <depend package="ogre_tools"/>
  <depend package="tf"/>
  <depend package="geometry_msgs"/>
  <depend package="sensor_msgs"/>
  <depend package="message_filters"/>
  <depend package="cob_script_server"/>
  <depend package="actionlib"/>
  <depend package="actionlib_msgs"/>

