<?xml version="1.0"?>
<launch>

	<!-- start platform node -->
	<node pkg="cob_platform" type="cob_platform" name="cob_platform" cwd="node" output="screen"/>

	<!-- teleop keyboard node -->
	<node pkg="pr2_teleop" type="teleop_pr2_keyboard" name="spawn_teleop_keyboard" output="screen">
		<param name="walk_vel" value="0.2" />
		<param name="run_vel" value="0.3" />
		<param name="yaw_rate" value="0.2" />
		<param name="yaw_run_rate" value="0.3" />
	</node>
    
</launch>
